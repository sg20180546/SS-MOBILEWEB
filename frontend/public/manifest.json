{
  "name": "서담서치",
  "permissions": [
    "storage"
  ],
  "description": "서담 검색 익스엔션 서담서치",
  "version": "1.0.3",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "서담서치"
  },
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}